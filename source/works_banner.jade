extend layout.jade

block popin
    .dropmenu_space#menu_content_banner
        ul.kind_lists_banner
            li.list(data-value="all").segment-0
                a(href="javascript:void(0)",data-value="all").active ALL
            li.list.segment-1
                a(href="javascript:void(0)",data-value="work") RECRUIT
            li.list.segment-2
                a(href="javascript:void(0)",data-value="test") TEST-AD
            li.list.segment-3
                a(href="javascript:void(0)",data-value="act") ACTIVITY
            //- li.list.segment-4
            //-     a(href="javascript:void(0)",data-value="travel") TRAVEL
            li.list.segment-5
                a(href="javascript:void(0)",data-value="pd") PRODUCT
            li.list.segment-6
                a(href="javascript:void(0)",data-value="game") GAME
            li.list.segment-7
                a(href="javascript:void(0)",data-value="holiday") HOLIDAY
        .cancel_btn
            img(src="./images/cancel.png", alt="")
    .nav_mobile
        ul
            li.nav_list
                a(href="./index.html")  ABOUT
            li.nav_list
                a(href="javascript:void(0)")  WORKS:
                .works_list
                    a.nav_work.nav_work_web(href="./works_cv.html") 履歷健診
                    a.nav_work.nav_work_web(href="./works_vitoas.html") 蜜得絲 商品導購頁
                    a.nav_work.nav_work_web(href="./works_bifi.html") 比菲德氏菌 導購頁
                    a.nav_work.nav_work_web(href="./works_nijigen.html") NIJIGEN 官方網頁
                    a.nav_work.nav_work_banner(href="./works_banner.html") Banner Collection
                    a.nav_work.nav_work_banner(href="./works_graphic.html") 平面印刷品
                    a.nav_work.nav_work_web(href="./works_naked.html") 裸雀小酒館 活動頁
            li.nav_list
                a(href="./images/resume.pdf")  RESUME
        .cancel_btn
            img(src="./images/cancel.png", alt="")
