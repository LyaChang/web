extend layout.jade

block loading
    -var borderColor = 'pink'
    include ./partials/loading.jade

block borders
    .top_border_pink
    .bottom_border_pink
    .right_border_pink
    .left_border_pink

block navbar
    -var path = 'works.html'
    -var logoColor = 'pink'
    -var nav_bg= ''
    -var togglerColor = ''
    include ./partials/navbar.jade

block content
    .works_section1
        .container
            .row.justify-content-center
                .col-11.col-sm-12
                    h1.banner_title Banner Collection
                .col-11.col-sm-12
                    ul.kind_lists.kind_lists_banner
                        li.list_kind(data-value="all").segment-0
                            a(href="#",data-value="all_banner").active All
                                span (22)
                        li.list_kind.segment-1
                            a(href="#",data-value="work") 工作轉職
                                span (7)
                        li.list_kind.segment-2
                            a(href="#",data-value="test") 測驗分析
                                span (6)
                        li.list_kind.segment-3
                            a(href="#",data-value="act") 活動
                                span (8)
                        li.list_kind.segment-4
                            a(href="#",data-value="travel") 旅遊
                                span (1)
